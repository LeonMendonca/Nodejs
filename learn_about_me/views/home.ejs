<%- include ('header') -%>
  <h1>Welcome to Learn About Me!</h1>
  <% users.forEach(function(user) { %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <a href="/users/<%= user._id.toString() %>">
          <%= user.displayName %>
        </a>
      </div>
      <% if (user.bio) { %>
        <div class="panel-body"><%= user.bio %></div>
      <% } %>
    </div>
  <% }) %>
<%- include ('footer') -%>
